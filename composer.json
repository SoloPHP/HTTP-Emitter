{
  "name": "solophp/http-emitter",
  "type": "library",
  "description": "PSR-7 HTTP Response Emitter",
  "keywords": ["php","psr-7","http","response","emitter"],
  "license": "MIT",
  "authors": [
    {
      "name": "Vitaliy Olos",
      "email": "vedastudio@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Solo\\HttpEmitter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Solo\\HttpEmitter\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "psr/http-message": "^1.1|^2.0",
    "solophp/contracts": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "phpstan/phpstan": "^2.0.0",
    "squizlabs/php_codesniffer": "^4.0"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR12 src/ tests/",
    "cs-fix": "phpcbf --standard=PSR12 src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
